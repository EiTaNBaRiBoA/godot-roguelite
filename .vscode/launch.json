{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "project (win | vsdbg)",
      "type": "cppvsdbg",
      "request": "launch",
      "requireExactSource": true,
      "symbolOptions": {
        "searchPaths": [
          "${workspaceFolder}/extern/godot-engine/bin",
          "${workspaceFolder}/project"
        ],
        "searchMicrosoftSymbolServer": true
      },
      "program": "unsupported platform, add executable path to launch.json",
      "windows": {
        "program": "${workspaceFolder}/extern/godot-engine/bin/godot.windows.editor.dev.x86_64.exe"
      },
      "linux": {
        "program": "${workspaceFolder}/extern/godot-engine/bin/godot.linuxbsd.editor.dev.x86_64"
      },
      "args": [
        "--debug",
        "--verbose",
        "--path",
        "${workspaceFolder}/project",
        "res://main.tscn"
      ],
      "cwd": "${workspaceFolder}/project",
      "visualizerFile": "${workspaceFolder}/extern/godot-engine/platform/windows/godot.natvis",
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "externalConsole": false,
    },
    {
      "name": "editor (win | vsdbg)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "unsupported platform, add executable path to launch.json",
      "windows": {
        "program": "${workspaceFolder}/extern/godot-engine/bin/godot.windows.editor.dev.x86_64.exe",
      },
      "linux": {
        "program": "${workspaceFolder}/extern/godot-engine/bin/godot.linuxbsd.editor.dev.x86_64",
      },
      "args": [
        "--editor",
        "--debug",
        "--verbose",
        "--path",
        "${workspaceFolder}/project"
      ],
      "cwd": "${workspaceFolder}/project",
      "visualizerFile": "${workspaceFolder}/extern/godot-engine/platform/windows/godot.natvis",
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal",
      "externalConsole": false,
    },
    {
      "name": "project (linux | gdb)",
      "type": "cppdbg",
      "request": "launch",
      "MIMode": "gdb",
      "program": "unsupported platform, add executable path to launch.json",
      "linux": {
        "program": "${workspaceFolder}/extern/godot-engine/bin/godot.linuxbsd.editor.dev.x86_64",
        "miDebuggerPath": "/usr/bin/gdb"
      },
      "args": [
        "--debug",
        "--verbose",
        "--path",
        "${workspaceFolder}/project",
        "res://main.tscn"
      ],
      "cwd": "${workspaceFolder}/project",
      "visualizerFile": "${workspaceFolder}/extern/godot-engine/platform/windows/godot.natvis",
      "internalConsoleOptions": "openOnSessionStart",
      "externalConsole": false,
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "editor (linux | gdb)",
      "type": "cppdbg",
      "request": "launch",
      "MIMode": "gdb",
      "program": "unsupported platform, add executable path to launch.json",
      "linux": {
        "program": "${workspaceFolder}/extern/godot-engine/bin/godot.linuxbsd.editor.dev.x86_64",
        "miDebuggerPath": "/usr/bin/gdb"
      },
      "args": [
        "--editor",
        "--debug",
        "--verbose",
        "--path",
        "${workspaceFolder}/project"
      ],
      "cwd": "${workspaceFolder}/project",
      "visualizerFile": "${workspaceFolder}/extern/godot-engine/platform/windows/godot.natvis",
      "internalConsoleOptions": "openOnSessionStart",
      "externalConsole": false,
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
