{
  "version": "0.2.1",
  "configurations": [
    {
      "type": "dll",
      "exe": "${workspaceRoot}\\godot\\engine\\bin\\godot.windows.editor.dev.x86_64.exe",
      "args": [
        "--editor",
        "--windowed",
        "--delta-smoothing enable",
        "--resolution 1920x1080",
        "--debug",
        "--verbose",
        "--path",
        "${workspaceRoot}\\roguelite"
      ],
      "project": "CMakeLists.txt",
      "name": "gdextension (editor)"
    },
    {
      "type": "dll",
      "isDefaultConfiguration": true,
      "exe": "${workspaceRoot}\\godot\\engine\\bin\\godot.windows.editor.dev.x86_64.exe",
      "args": [
        "--debug",
        "--verbose",
        "--path",
        "${workspaceRoot}\\roguelite",
        "res://main.tscn"
      ],
      "project": "CMakeLists.txt",
      "name": "gdextension (console)"
    }
  ]
}
